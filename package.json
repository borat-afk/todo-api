{
  "name": "todo-api",
  "version": "1.0.0",
  "description": "API for TODO List",
  "main": "dist/server.js",
  "prestart": "npm run build",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-esm --experimental-specifier-resolution=node --transpile-only src/server.ts",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run": "npm run typeorm migration:run -- -d ./src/dataSource.js",
    "typeorm:generate": "npm run typeorm -- -d ./src/dataSource.js migration:generate src/migrations/%npm_config_name%",
    "typeorm:create": "npm run typeorm -- migration:create src/migrations/%npm_config_name%",
    "typeorm:revert": "npm run typeorm -- -d ./src/dataSource.js migration:revert"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borat-afk/todo-api.git"
  },
  "author": "Vitalii Chernenko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/borat-afk/todo-api/issues"
  },
  "homepage": "https://github.com/borat-afk/todo-api#readme",
  "dependencies": {
    "@types/express": "^4.17.17",
    "cors": "^2.8.5",
    "docker-compose": "^0.24.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-fetch": "^2.6.12",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.17",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "i": "^0.3.7",
    "nodemon": "^3.0.1",
    "npm": "^9.8.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0"
  }
}
